<script type="text/javascript">

	$(".white").addClass("active");
	$(".black").removeClass("active");
	$(".header-block .navbar-nav > li > a").css("color", "white");

	if($(window).width() < 767) {
	$(".header-block .navbar-nav > li > a").css("color", "#616195");
	}
	
	$(window).scroll(function() {    
		var scroll = $(window).scrollTop();
		
		 //>=, not <=
		if (scroll >= 50) {

			$(".header-block").addClass("darkHeader");
			$(".white").removeClass("active");
			$(".black").addClass("active");
			$(".header-block .navbar-nav > li > a").css("color", "#616195");
			
		} else {
			$(".header-block").removeClass("darkHeader");
			$(".white").addClass("active");
			$(".black").removeClass("active");
			$(".header-block .navbar-nav > li > a").css("color", "white");

			
		}
		AOS.init({
        offset: 200,
      duration: 600,
      easing: 'ease-in-sine',
      delay: 100,
      });
	}); 
	

	

</script>
