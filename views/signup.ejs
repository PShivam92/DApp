<% if (message.length > 0) { %>
<div class="alert alert-danger col-sm-12"><%= message %></div><% } %>
        
            <div class="content-frame">
                <h2>Sign Up</h2>
                <form id="regform"  method="post" action="/signup">
                    <div class="field-block">
                        <div class="group">
                            <input type="text" name="first_name" id="inputFirstName"value="" required="required" autofocus="">
                            <label>First Name</label>
                        </div>
                    </div>
                    <div class="field-block">
                        <div class="group">
                            <input type="text" name="last_name" id="inputLastName" value="" required="required">
                            <label>Last Name</label>
                        </div>
                    </div>
                    <div class="field-block">
                        <div class="group">
                              <input type="text" name="user_name" id="username" value="" required="required">
                            <label>User Name</label>
                        </div>
                    </div>
                    <div class="field-block">
                        <div class="group">
                            <input type="email" name="email_id" id="inputEmail" maxlength="60" value="" required="required">
                            <label>Email </label>
                        </div>
                    </div>
<!--                     <div class="field-block">
                        <div class="group">
                            <input type="tel" name="account_id" id="metamask" class="field" maxlength="60" value="" required="">
                            <label>Meta mask account address</label>
                        </div>
                    </div> -->
                    <div class="field-block">
                        <div class="group">
                            <input type="number" name="mob_no" id="inputPhone" class="field" value="" required="">
                            <label>Phone number</label>
                            <strong id="phoneerror" style="color:red"></strong>
                        </div>
                    </div>
                    <div class="field-block">
                        <div class="group">
                            <input type="password" name="password" id="inputNewPassword1" autocomplete="off" required="required">
                            <label>Password</label>
                        </div>
                    </div>
                    <div class="field-block">
                        <div class="group">
                            <input type="password" name="password2" id="inputNewPassword2" autocomplete="off" required="required">
                            <label>Re-enter password</label>
                            <strong id="pwderror" style="color:red"></strong>
                        </div>
                    </div>
                    <div class="form-group">
                        <button type="submit" class="btn btn-default login-btn">Create Account</button>
                    </div>
                    <div class="text-strikethru">
                        <div class="line"></div>
                        <div class="text">or</div>
                    </div>
                    <div class="form-group">
                        <button type="submit" class="btn btn-default fb-btn"><i><img src="/static/images/facebook-icon.png"></i> Continue with Facebook</button>
                    </div>
                </form>
            </div>
