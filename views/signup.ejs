<script type="text/javascript">
$(".inner-header .navbar-nav > li > a").css("color", "#616195");

    $(".group input").each(function(){
var input_value = $(this).val().length;
if(input_value > 0){
          $(this).addClass('valid');
          if($(this).val() == ""){$(this).removeClass('valid');}
 }
else
{
       $(this).removeClass('valid');
 }
});

$(".group input").keyup(function(){
    
    var input_value1 = $(this).val().length;
    if( input_value1 > 0){
        $(this).addClass('valid');
    }
    else
    {
        $(this).removeClass('valid');
    }
});


</script>
<!-- validations signup  -->
<script type="text/javascript">
  
    $('#regform').submit(function() {
            var password =  document.getElementById("inputNewPassword1").value;
          var confirmpassword = document.getElementById("inputNewPassword2").value;
          var phoneno = document.getElementById("inputPhone").value;
        
           if(!(phoneno.length==10)){
           document.getElementById("phoneerror").innerHTML="*Invalid Phone Number";
            return false;
          }
          else if(password!=confirmpassword) {
            document.getElementById("pwderror").innerHTML="*Passwords Didn't Match";
            return false;
          }
          else{
            return true;
          }
});
</script>
