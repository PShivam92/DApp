  <script type="text/javascript" src="js/bootstrap-filestyle.js"></script>
  <script>
  $(document).ready(function()
  {
    $(".uploadButton").click(function()
    {
      $("#uploadForm").trigger("reset");
      $(".uploadBox").hide();
    });

    $('input[type="radio"]').click(function()
    {
      if($(this).attr("value")=="gravatar")
      {
        $(".uploadBox").not(".gravatar").hide();
        $(".gravatar").show();
      }

      if($(this).attr("value")=="computer")
      {
        $(".uploadBox").not(".computer").hide();
        $(".computer").show();
      }

      if($(this).attr("value")=="url")
      {
        $(".uploadBox").not(".url").hide();
        $(".url").show();
      }
    });
  });

  $("#uploadForm").submit(function(e)
  {
    var form = $(this);
    var formdata = false;
    if(window.FormData)
    {
      formdata = new FormData(form[0]);
    }

    var formAction = form.attr("action");

    $.ajax({
        type        : "POST",
        url         : "modules/addons/ProfilePicture/data.php",
        cache       : false,
        data        : formdata ? formdata : form.serialize(),
        contentType : false,
        processData : false,

        success: function(response)
        {
          if(response == "success")
          {
            $("#uploadImageModal").modal("hide");
            location.reload();
          }
          else
          {
            $("#uploadMessages").addClass("alert alert-danger").text(response).slideDown().delay(2500).slideUp();
          }
       }
    });
    e.preventDefault();
  });
  </script>
